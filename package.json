{
  "name": "typed-intersect-difference",
  "version": "2.0.1",
  "description": "generic, typed intersection/difference of sorted arrays of unique objects",
  "keywords": [
    "intersect",
    "difference"
  ],
  "files": [
    "index.ts"
  ],
  "main": "./index.ts",
  "types": "./index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "author": "Derek Berger <dmberger.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DM-Berger/typed-intersect-difference"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  }
}
